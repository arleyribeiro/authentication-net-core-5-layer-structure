    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch Development",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Authentication/bin/Debug/net5.0/Authentication.dll",
            "args": [],
            "cwd": "${workspaceFolder}\\Authentication",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)\\swagger\\index.html"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Authentication/bin/Debug/net5.0/Authentication.dll",
            "args": [],
            "cwd": "${workspaceFolder}\\Authentication",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}